<html>
    <head>
        <meta charset="utf-8">
    </head>
    <body>
        <h1> Livros </h1>
        <table>
            <thead>
                <tr>
                    <th>ID</th>
                    <th>Title</th>
                </tr>
            </thead>
            <tbody>
                <tr for(livro in data.livros)>
                    <td>${livro.id}</td>
                    <td>${livro.title}</td>
                </tr>
            </tbody>
        </table>
    </body>
</html>