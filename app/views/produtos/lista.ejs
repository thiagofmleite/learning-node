<html>

<body>
    <table>
        <thead>
            <tr>
                <th>Id</th>
                <th>Titulo</th>
                <th>Descricao</th>
                <th>Preco</th>
            </tr>
        </thead>
        <tbody>
            <%for(var i=0;i<lista.length;i++) { %>
            <tr>
                <td><%=lista[i].id%></td>
                <td><%=lista[i].titulo%></td>
                <td><%=lista[i].descricao%></td>
                <td><%=lista[i].preco%></td>
            </tr>
            <% } %>
        </tbody>
    </table>
</body>

</html>